  <data name="EmailHtmlTemplate" xml:space="preserve">
    <value>&lt;html&gt;
&lt;head&gt;
    &lt;title&gt;$SUBJECT$&lt;/title&gt;
    &lt;style type="text/css"&gt;
        .email-body {
            font-family: Verdana, Geneva, Tahoma, sans-serif;
            font-size: 12pt;
            width: 100%;
            height: 100%;
            background-color: #fbf3b7;
        }

        .email-content {
            width: 600px;
            margin: 20px;
            padding: 20px;
            background-color: #ffffff;
        }

        .email-header {
            text-align: center;
            padding-bottom: 15px;
        }

        .email-heading {
            font-size: 2rem;
            text-align: center;
            padding-bottom: 15px;
        }

        .email-prewrap {
            width: 600px;
            white-space: pre-wrap; /* Since CSS 2.1 */
            white-space: -moz-pre-wrap; /* Mozilla, since 1999 */
            white-space: -pre-wrap; /* Opera 4-6 */
            white-space: -o-pre-wrap; /* Opera 7 */
            word-wrap: break-word; /* Internet Explorer 5.5+ */
        }
    &lt;/style&gt;
&lt;/head&gt;
&lt;body style="border: 0; margin: 0; padding: 0;"&gt;
    &lt;table role="presentation" border="0" cellspacing="0" cellpadding="0" class="email-body"&gt;
        &lt;tr&gt;
            &lt;td align="center" valign="top"&gt;
                &lt;table role="presentation" border="0" cellspacing="0" cellpadding="0" class="email-content"&gt;
                    &lt;tr&gt;
                        &lt;td class="email-header"&gt;&lt;img src="$FULL-URL$/images/logo-email.gif" /&gt;&lt;/td&gt;
                    &lt;/tr&gt;
                    &lt;tr&gt;
                        &lt;td class="email-heading"&gt;$SUBJECT$&lt;/td&gt;
                    &lt;/tr&gt;
                    &lt;tr&gt;
                        &lt;td&gt;
                            $TEMPLATE-BODY$
                        &lt;/td&gt;
                    &lt;/tr&gt;
                &lt;/table&gt;
            &lt;/td&gt;
        &lt;/tr&gt;
    &lt;/table&gt;
&lt;/body&gt;
&lt;/html&gt;</value>
  </data>
